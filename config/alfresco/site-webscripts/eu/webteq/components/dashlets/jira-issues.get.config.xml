<config>
	<jiraUser></jiraUser>
	<pageSize>100</pageSize>
	<!-- Set of columns to display to the user -->
	<columns>key,duedate,summary</columns>
	<filters>
		<filter label="filter.allIssues" value="all"/>
		<filter label="filter.overdue" value="overdue"/>
		<filter label="filter.dueToday" value="today"/>
		<filter label="filter.dueNextFiveDays" value="next"/>
	</filters>
	<jiraUrl>http://localhost:8090</jiraUrl>
</config>